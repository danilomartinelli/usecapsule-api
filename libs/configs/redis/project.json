{
  "name": "@usecapsule/redis",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/configs/redis/src",
  "projectType": "library",
  "tags": ["scope:config", "type:lib", "layer:infrastructure"],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/configs/redis",
        "tsConfig": "libs/configs/redis/tsconfig.lib.json",
        "packageJson": "libs/configs/redis/package.json",
        "main": "libs/configs/redis/src/index.ts",
        "assets": ["libs/configs/redis/*.md"],
        "rootDir": "libs/configs/redis/src"
      }
    },
    "copy-declarations": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "mkdir -p libs/configs/redis/dist",
          "cp -r dist/libs/configs/redis/* libs/configs/redis/dist/ 2>/dev/null || true"
        ],
        "parallel": false
      }
    }
  }
}
